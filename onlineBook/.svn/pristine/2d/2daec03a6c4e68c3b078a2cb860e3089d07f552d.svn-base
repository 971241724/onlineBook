package com.b2c.entity;
/**
 * 
 * 管理员实体类
 * @author 高欢
 * createTime 2017/08/17
 */
public class Admin {
	
	private int admin_id;
	private String admin_no;
	private String admin_pwd;
	private String admin_name;
	private int admin_age;
	private String admin_sex;
	public int getAdmin_id() {
		return admin_id;
	}
	public void setAdmin_id(int admin_id) {
		this.admin_id = admin_id;
	}
	public String getAdmin_no() {
		return admin_no;
	}
	public void setAdmin_no(String admin_no) {
		this.admin_no = admin_no;
	}
	public String getAdmin_pwd() {
		return admin_pwd;
	}
	public void setAdmin_pwd(String admin_pwd) {
		this.admin_pwd = admin_pwd;
	}
	public String getAdmin_name() {
		return admin_name;
	}
	public void setAdmin_name(String admin_name) {
		this.admin_name = admin_name;
	}
	public int getAdmin_age() {
		return admin_age;
	}
	public void setAdmin_age(int admin_age) {
		this.admin_age = admin_age;
	}
	public String getAdmin_sex() {
		return admin_sex;
	}
	public void setAdmin_sex(String admin_sex) {
		this.admin_sex = admin_sex;
	}
	public Admin(int admin_id, String admin_no, String admin_pwd) {
		super();
		this.admin_id = admin_id;
		this.admin_no = admin_no;
		this.admin_pwd = admin_pwd;
	}
	
	public Admin() {
		super();
	}
	@Override
	public String toString() {
		return "Admin [admin_id=" + admin_id + ", admin_no=" + admin_no
				+ ", admin_pwd=" + admin_pwd + ", admin_name=" + admin_name
				+ ", admin_age=" + admin_age + ", admin_sex=" + admin_sex + "]";
	}
	public Admin(int admin_id, String admin_no, String admin_pwd,
			String admin_name, int admin_age, String admin_sex) {
		super();
		this.admin_id = admin_id;
		this.admin_no = admin_no;
		this.admin_pwd = admin_pwd;
		this.admin_name = admin_name;
		this.admin_age = admin_age;
		this.admin_sex = admin_sex;
	}
}
