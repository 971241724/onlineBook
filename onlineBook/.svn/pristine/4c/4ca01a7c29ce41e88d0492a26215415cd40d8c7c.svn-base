package com.b2c.entity;

public class Cart {
	
	public static final String TABLENAME="¹ºÎï³µ±í";
	
	private int cart_id;
	private Book book;
	private int user_id;
	private int amount;
	private Double c_price;
	private int book_id;
	public int getCart_id() {
		return cart_id;
	}
	public void setCart_id(int cart_id) {
		this.cart_id = cart_id;
	}
	public Book getBook() {
		return book;
	}
	public void setBook(Book book) {
		this.book = book;
	}
	public int getUser_id() {
		return user_id;
	}
	public void setUser_id(int user_id) {
		this.user_id = user_id;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	
	public Double getC_price() {
		return c_price;
	}
	public void setC_price(Double c_price) {
		this.c_price = c_price;
	}
	public int getBook_id() {
		return book_id;
	}
	public void setBook_id(int book_id) {
		this.book_id = book_id;
	}
	public Cart() {
		super();
	}
	public Cart(int cart_id, Book book, int user_id, int amount,
			Double c_price, int book_id) {
		super();
		this.cart_id = cart_id;
		this.book = book;
		this.user_id = user_id;
		this.amount = amount;
		this.c_price = c_price;
		this.book_id = book_id;
	}
	@Override
	public String toString() {
		return "Cart [cart_id=" + cart_id + ", book=" + book + ", user_id="
				+ user_id + ", amount=" + amount + ", c_price=" + c_price
				+ ", book_id=" + book_id + "]";
	}
	
}
